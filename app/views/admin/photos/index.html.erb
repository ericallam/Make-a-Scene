<h1>Photos for <%= @event.name %></h1>


<% @photos.each do |photo| %>

  <div>
    <%= image_tag photo.image.url(:medium) %> <%= link_to 'Delete', admin_event_photo_path(@event, photo), :method => :delete, :confirm => "Are you sure you want to delete this photo?" %>
  </div>
  
<% end %>

<%= link_to 'Upload Photos', new_admin_event_photo_upload_path(@event) %>
